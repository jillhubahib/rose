{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedParameters": false,
		"noUnusedLocals": false,
		"sourceMap": true
	}
}