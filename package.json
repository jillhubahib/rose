{
  "name": "rose",
  "version": "0.0.1",
  "description": "Type-safe query builder DSL, data mapper, and mini ORM for PostgreSQL",
  "main": "index.js",
  "bin": {
    "rose": "./cli/introspect.js"
  },
  "scripts": {
    "compile": "tsc",
    "explore": "node build/src/cli/explore.js",
    "genFuncs": "node build/tools/pgDocsParsing.js",
    "lint": "eslint . --ext .ts",
    "package": "tsc -p tsconfig_package.json && ts-node tools/package.ts",
    "test": "mocha",
    "watch": "tsc -w --pretty"
  },
  "keywords": [],
  "author": "Laurence Dougal Myers <laurencedougalmyers@gmail.com>",
  "license": "MIT",
  "engines": {
    "yarn": "1.x"
  },
  "dependencies": {
    "camelcase": "5.3.1",
    "lodash.clonedeep": "4.5.0",
    "metrohash": "2.6.0",
    "outdent": "0.7.1",
    "pg": "6.1.0"
  },
  "devDependencies": {
    "@types/app-root-path": "1.2.4",
    "@types/lodash.clonedeep": "4.5.3",
    "@types/mocha": "7.0.1",
    "@types/node": "12",
    "@types/pg": "6.1.34",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "app-root-path": "3.0.0",
    "eslint": "6.8.0",
    "eslint-plugin-unused-imports": "0.1.2",
    "mocha": "7.0.1",
    "ts-node": "8.8.1",
    "typescript": "3.8.3"
  }
}
