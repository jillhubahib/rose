{
  "name": "@rosepg/rose",
  "version": "0.0.17",
  "description": "Type-safe query builder DSL, data mapper, and mini ORM for PostgreSQL",
  "main": "index.js",
  "scripts": {
    "check:all": "yarn run compile --noEmit && yarn run lint && yarn run test",
    "compile": "tsc",
    "genFuncs": "ts-node tools/pgDocsParsing.ts",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "package": "rimraf ./package && tsc -p tsconfig_package.json && ts-node tools/package.ts",
    "publish:npm": "yarn run check:all && yarn run package && cd package && yarn publish --non-interactive --access public",
    "test": "nyc mocha",
    "watch": "tsc -w --pretty"
  },
  "keywords": [],
  "author": "Laurence Dougal Myers <laurencedougalmyers@gmail.com>",
  "license": "MIT",
  "engines": {
    "yarn": "1.x"
  },
  "dependencies": {
    "lodash.clonedeep": "4.5.0",
    "metrohash": "2.6.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/app-root-path": "1.2.4",
    "@types/lodash.clonedeep": "4.5.3",
    "@types/mocha": "7.0.1",
    "@types/node": "12",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "app-root-path": "3.0.0",
    "eslint": "6.8.0",
    "eslint-plugin-unused-imports": "0.1.2",
    "mocha": "7.0.1",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "source-map-support": "0.5.19",
    "ts-node": "8.8.1",
    "typescript": "3.8.3"
  }
}
