{
  "name": "@rosepg/rose-docs",
  "version": "0.0.17",
  "description": "Documentation for rose",
  "main": "index.js",
  "author": "Laurence Dougal Myers <laurencedougalmyers@gmail.com>",
  "license": "MIT",
  "scripts": {
    "docker:start": "docker run --name rose-docs-db -p 5443:5432 -d mujz/pagila",
    "docker:stop": "docker stop rose-docs-db && docker rm rose-docs-db",
    "introspect": "rimraf ./generated/ && rose --url postgresql://root:admin@localhost:5443/pagila --out generated\\db",
    "link:rose": "yarn link @rosepg/rose @rosepg/rose-cli",
    "lint": "eslint ./docs/modules/ROOT/examples/test/ --ext .ts --max-warnings 0",
    "start": "rimraf ./build/ && antora antora-playbook.yml",
    "test": "mocha"
  },
  "dependencies": {
    "@antora/cli": "2.3.4",
    "@antora/site-generator-default": "2.3.4",
    "app-root-path": "3.0.0",
    "asciidoctor": "2.2.0"
  },
  "devDependencies": {
    "@types/app-root-path": "1.2.4",
    "@types/mocha": "7.0.2",
    "@types/node": "12",
    "@types/pg": "7.14.3",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "eslint": "7.14.0",
    "mocha": "7.1.1",
    "pg": "8.0.0",
    "rimraf": "3.0.2",
    "ts-node": "9.0.0",
    "typescript": "4.1.2"
  }
}
